@UPDATE

CALL ADD_ALL_CHARA

FOR LOCAL, 1, CHARANUM
    SELECTCASE NO:LOCAL
        CASE 7, 12, 32, 51, 68, 89, 113, 123, 132, 138
            CFLAG:LOCAL:개별그림유무 = 1
    ENDSELECT
NEXT
